{
    "name": "e282486518/laravel-api-database",
    "description": "使用API代替数据库,和使用MySQL一样使用,适用于第三方数据库或者连接数组中心",
    "keywords": [
        "laravel",
        "database",
        "api",
        "eloquent"
    ],
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "e282486518",
            "email": "phphome@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "e282486518\\LaravelApiDatabase\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "e282486518\\LaravelApiDatabase\\ApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
